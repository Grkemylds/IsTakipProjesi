@using MvsFirmaCagri1.Models.Entity
@model List<TblCagrıDetay>
@{
    ViewBag.Title = "CagriDetay";
    Layout = "~/Views/Shared/FirmaLayout.cshtml";
}
<h3>Çağrı Detayları</h3>
<table class="table table-bordered">
    <div>
    <tr>
        <th>#</th>
        <th>Açıklama</th>
        <th>Tarih</th>
        <th>Saat</th>
    </tr></div>
    @foreach (var x in Model)
    {
        <tr>
            <td>@x.ID</td>
            <td>@x.Açıklama</td>
            <td>@x.Tarih</td>
            <td>@x.Saat</td>

        </tr>
    }
</table>

